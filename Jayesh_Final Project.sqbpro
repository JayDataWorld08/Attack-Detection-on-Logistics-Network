    Address_ID INTEGER PRIMARY KEY,
    SeedAddress TEXT,
    ExpAddress TEXT,
    IPAddress TEXT
);

CREATE TABLE Logistics (
    Logistics_ID INTEGER PRIMARY KEY,
    Geolocation TEXT,
    NumberOfPackets INTEGER,
    PayloadSize INTEGER
);

CREATE TABLE Network_Traffic (
    Traffic_ID INTEGER PRIMARY KEY,
    Time TIMESTAMP,
    Protocol TEXT,
    Flag TEXT,
    ThreatLevel INTEGER,
    AnomalyScore REAL,
    ResponseTime INTEGER,
    DataTransferRate INTEGER,
    FK_Logistics_ID INTEGER,
    FK_Address_ID INTEGER,
    FOREIGN KEY (FK_Logistics_ID) REFERENCES Logistics(Logistics_ID),
    FOREIGN KEY (FK_Address_ID) REFERENCES Address(Address_ID)
);

CREATE TABLE Session (
    Session_ID INTEGER PRIMARY KEY,
    FK_Traffic_ID INTEGER,
    FOREIGN KEY (FK_Traffic_ID) REFERENCES Network_Traffic(Traffic_ID)
);

CREATE TABLE Network_Event (
    EventDescription_ID INTEGER PRIMARY KEY,
    UserAgent TEXT,
    ErrorCode INTEGER,
    FK_Traffic_ID INTEGER,
    FOREIGN KEY (FK_Traffic_ID) REFERENCES Network_Traffic(Traffic_ID)
);



SELECT 
    Time, Protocol, Flag, ThreatLevel, AnomalyScore, 
    SeedAddress, ExpAddress, IPAddress,
    Geolocation, PayloadSize, NumberofPackets
FROM 
    Network_Traffic nt
JOIN 
    Address addr ON Traffic_ID = Traffic_ID
JOIN 
    Logistics log ON nt.FK_Logistics_ID = Logistics_ID
WHERE 
    ThreatLevel &gt; 1 AND AnomalyScore &gt; 0.07
	

SELECT 
    Time, ThreatLevel, EventDescription_ID, ErrorCode,
    AnomalyScore, ResponseTime, Session_ID
FROM 
    Network_Traffic nt
JOIN 
    Network_Event ne ON Traffic_ID = Traffic_ID
JOIN 
    Session sess ON Traffic_ID = Traffic_ID
WHERE 
    AnomalyScore &gt; 0.9 OR ErrorCode IS NOT NULL;

	
	
SELECT 
    Time, Protocol, DataTransferRate, AnomalyScore, 
    SeedAddress, ExpAddress, ThreatLevel
FROM 
    Network_Traffic nt
JOIN 
    Address addr ON Traffic_ID = Traffic_ID
WHERE 
    DataTransferRate &gt; 10.50 AND Protocol = 'TCP'
ORDER BY 
    AnomalyScore DESC;

	
	
	
SELECT 
    Time, Protocol, DataTransferRate, ThreatLevel,
    SeedAddress, ExpAddress, PayloadSize, NumberofPackets
FROM 
    Network_Traffic nt
JOIN 
    Address addr ON Traffic_ID = Traffic_ID
JOIN 
    Logistics log ON Logistics_ID = Logistics_ID
WHERE 
    nt.Protocol IN ('UDP', 'TCP') 
    AND DataTransferRate &gt; 17.20
    AND ThreatLevel &gt; 1;

	
	
	
SELECT
	Logistics_id, Session, Time, Response_Time, Prediction, Anomaly_Score
FROM
	Network_Traffic
WHERE
	Anomaly Score &gt; 70 AND Prediction = 'Disruption'
ORDER BY
	Response_Time DESC;









